{
  "name": "audioground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "run-script-os",
    "test:darwin": "npm-run-all --parallel test:default:*",
    "test:default:rust": "(cd src-tauri && cargo test)",
    "test:default:py": "python -m unittest discover -s src-tauri/python -p '*_test.py'",
    "wdio": "wdio run ./wdio.conf.ts",
    "prepare": "husky",
    "download:models": "(cd src-tauri/python && python -c 'from labeler import download_defaults; download_defaults()')"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@tauri-apps/api": "^1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.1",
    "@wdio/browser-runner": "^8.40.2",
    "@wdio/cli": "^8.40.2",
    "@wdio/globals": "^8.40.2",
    "@wdio/mocha-framework": "^8.40.2",
    "@wdio/spec-reporter": "^8.39.0",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  }
}
